{
    "name": "chariot_chrome_extension",
    "version": "1.0.0",
    "manifest_version": 2,
    "description":"@author Xi Zhu(A00372176)",
    "browser_action": {
        "default_title": "Chariot",
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },    
    "permissions": [
        "background",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]   
}
